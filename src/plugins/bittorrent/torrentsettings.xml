				<item type="groupbox" property="OverridePeerIDVersion" default="false" checkable="true">
					<label lang="en" value="Override Peer ID version" />
					<label lang="ru" value="Переопределить версию Peer ID" />
					<label lang="uk" value="Задати версію Peer ID" />
