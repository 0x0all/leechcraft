						<lang value="en">
							<label value="Scrape interval" />
							<suffix value=" s" />
						</lang>
						<lang value="ru">
							<label value="Интервал scrape-запросов" />
							<suffix value=" с" />
						</lang>
						<lang value="uk">
							<label value="Інтервал scrape-запитів" />
							<suffix value=" с" />
						</lang>

					</item>
				</item>
