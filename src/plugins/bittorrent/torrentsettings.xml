						<label lang="en" value="Peer ID version:" />
						<label lang="ru" value="Версия Peer ID:" />
						<label lang="uk" value="Версія Peer ID:" />
					</item>
				</item>
			</groupbox>
		</tab>
		<tab>
			<label lang="en" value="Fast speed control" />
			<label lang="ru" value="Быстрый контроль скорости" />
			<label lang="uk" value="Спрощений контроль швидкості" />
			<item type="groupbox" property="EnableFastSpeedControl" checkable="true" state="off">
				<label lang="en" value="Enable fast speed control" />
				<label lang="ru" value="Включить быстрый контроль скорости" />
				<label lang="uk" value="Увімкнути спрощений контроль швидкості" />
				<item type="customwidget" name="FastSpeedControl" label="own" />
			</item>
		</tab>
		<tab>
			<label lang="en" value="Proxy" />
			<label lang="ru" value="Прокси" />
			<label lang="uk" value="Проксі" />
