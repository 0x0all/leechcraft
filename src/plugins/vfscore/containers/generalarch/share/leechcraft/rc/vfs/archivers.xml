<archivers>
	<arch type="ZIP">
		<archiver>
			<execName>unzip</execName>
			<cmdLine>
				<archName/>
				<arg>-d</arg>
				<destDir/>
			</cmdLine>
		</archiver>
	</arch>
	<arch type="TGZ">
		<archiver id="tar"/>
	</arch>
	<arch type="TBZ">
		<archiver id="tar"/>
	</arch>
	<arch type="TXZ">
		<archiver id="tar"/>
	</arch>
	<arch type="7ZIP">
		<archiver>
			<execName>7z</execName>
			<cmdLine>
				<arg>x</arg>
				<archName/>
				<arg>-o</arg>
				<destDir/>
			</cmdLine>
		</archiver>
	</arch>

	<archiver id="tar">
		<execName>tar</execName>
		<cmdLine>
			<arg>xf</arg>
			<archName/>
			<arg>-C</arg>
			<destDir/>
		</cmdLine>
	</archiver>
</archivers>