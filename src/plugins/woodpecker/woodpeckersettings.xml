<?xml version="1.0" encoding="UTF-8"?>
<settings>
  <page>
    <label lang="en" value="General settings" />
    <groupbox>
      <label value="Account" />
      <item type="lineedit" property="login" default="">
	<label lang="en" value="Login:" />
      </item>
      <item type="lineedit" property="password" default="">
	<label lang="en" value="Password:" />
      </item>
    </groupbox>
    <item type="lineedit" property="timer" default="90">
      <label lang="en" value="Update time, seconds" />
    </item>
  </page>
  <page>
    <label lang="en" value="Notification settings" />
    <item type="checkbox" property="notify" default="true">
      <label lang="en" value="Show notifications" />
    </item>
  </page>
</settings>
