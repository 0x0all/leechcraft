<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<page>
		<label value="Behavior" />
			<groupbox>
				<label value="Acetamide" />
				<item type="checkbox" property="AutoDisconnectFromServer" state="off">
						<label value="Autodisconnect from server when no channels are active" />
				</item>
				<item type="checkbox" property="ShowInviteDialog" state="off">
						<label value="Show invite dialog" />
				</item>
				<item type="checkbox" property="ServerDuplicateCommandAnswer" state="off">
					<label value="Duplicate answer on command in server's tab" />
				</item>
			</groupbox>
	</page>
	<page>
		<label value="NickServ" />
			<groupbox>
				<label value="NickServ identify" />
				<item type="customwidget" label="own" name="NickServIdentifyWidget" />
			</groupbox>
	</page>
</settings>
