<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<page>
		<label value="Azoth Tracolor" />
		<item type="checkbox" property="EnableTracolor" default="false">
			<label value="Enable Tracolor" />
		</item>
		<item type="spinbox" property="FadeoutTime" default="30" minimum="1" maximum="300">
			<label value="Fadeout time:" />
			<suffix value=" min" />
		</item>
		<item type="spinbox" property="HidingThreshold" default="5" minimum="1" maximum="255">
			<label value="Opacity threshold for icons hiding:" />
		</item>
		<item type="dataview" property="EventsTypesDataView">
		</item>
	</page>
</settings>
