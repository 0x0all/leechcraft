<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<declare defaultlang="en" />
	<page>
		<label lang="en" value="Browser settings" />
		<label lang="ru" value="Параметры браузера" />
		<tab>
			<label lang="en" value="Startup" />
			<label lang="ru" value="Запуск" />
			<item type="lineedit" property="DefaultPage" default="http://gmail.com">
				<label lang="en" value="Default page:" />
				<label lang="ru" value="Страница по умолчанию:" />
			</item>
			<item type="checkbox" property="RestorePreviousSession" state="on">
				<label lang="en" value="Restore previous session" />
				<label lang="ru" value="Восстанавливать предыдущую сессию" />
			</item>
		</tab>
		<tab>
			<item type="checkbox" property="BackgroundNewTabs" state="on">
				<label lang="en" value="Open new tabs in the background" />
				<label lang="ru" value="Открывать новые вкладки в фоне" />
			</item>
			<item type="checkbox" property="DifferLocationsByExtension" state="on">
				<label lang="en" value="Differ download locations by file extension" />
				<label lang="ru" value="Различать папки для скачивания по расширению файла" />
			</item>
			<label lang="en" value="Behavior" />
			<label lang="ru" value="Поведение" />
			<item type="lineedit" property="ExternalSchemes" default="smb nfs telnet imap pop nntp news">
				<label lang="en" value="Open these schemes using external applications:" />
				<label lang="ru" value="Открывать эти схемы внешними приложениями:" />
			</item>
		</tab>
	</page>
	<page>
		<label lang="en" value="Viewer settings" />
		<label lang="ru" value="Параметры просмотрщика" />
		<label lang="uk" value="Параметри переглядача" />
		<tab>
			<label lang="en" value="Behavior" />
			<label lang="ru" value="Поведение" />
			<label lang="uk" value="Поведінка" />
			<item type="checkbox" property="AutoLoadImages" state="on">
				<label lang="en" value="Load images automatically" />
				<label lang="ru" value="Автоматически загружать изображения" />
				<label lang="uk" value="Автоматично завантажувати зображення" />
			</item>
			<item type="checkbox" property="AllowJavaScript" state="on">
				<label lang="en" value="Allow JavaScript" />
				<label lang="ru" value="Разрешить JavaScript" />
				<label lang="uk" value="Дозволити JavaScript" />
			</item>
			<item type="path" property="UserStyleSheet">
				<label lang="en" value="User CSS" />
				<label lang="ru" value="Пользовательский CSS" />
			</item>
		</tab>
		<tab>
			<label lang="en" value="Fonts settings" />
			<label lang="ru" value="Настройки шрифтов" />
			<tab>
				<label lang="en" value="Fonts" />
				<label lang="ru" value="Шрифты" />
				<label lang="uk" value="Шрифти" />
				<item type="font" property="StandardFont">
					<label lang="en" value="Standard font:" />
					<label lang="ru" value="Стандартный шрифт:" />
					<label lang="uk" value="Стандартний шрифт:" />
				</item>
				<item type="font" property="FixedFont">
					<label lang="en" value="Fixed font:" />
					<label lang="ru" value="Моноширинный шрифт:" />
					<label lang="uk" value="Моноширинний шрифт:" />
				</item>
				<item type="font" property="SerifFont">
					<label lang="en" value="Serif font:" />
					<label lang="ru" value="Шрифт с засечками:" />
					<label lang="uk" value="Шрифт з зарубками:" />
				</item>
				<item type="font" property="SansSerifFont">
					<label lang="en" value="SansSerif font:" />
					<label lang="ru" value="Шрифт без засечек:" />
					<label lang="uk" value="Шрифт без зарубок:" />
				</item>
				<item type="font" property="CursiveFont">
					<label lang="en" value="Cursive font:" />
					<label lang="ru" value="Курсивный шрифт:" />
					<label lang="uk" value="Курсивний шрифт:" />
				</item>
				<item type="font" property="FantasyFont">
					<label lang="en" value="Fantasy font:" />
					<label lang="ru" value="Fantasy-шрифт:" />
					<label lang="uk" value="Fantasy-шрифт:" />
				</item>
			</tab>
			<tab>
				<label lang="en" value="Sizes" />
				<label lang="ru" value="Размеры" />
				<item type="spinbox" property="MinimumFontSize" default="9" minimum="5" maximum="40" step="1">
					<label lang="en" value="Minimum font size:" />
					<label lang="ru" value="Минимальный размер шрифта:" />
					<label lang="uk" value="Мінімальний розмір шрифта:" />
				</item>
				<item type="spinbox" property="DefaultFontSize" default="14" minimum="5" maximum="40" step="1">
					<label lang="en" value="Default font size:" />
					<label lang="ru" value="Размер шрифта по умолчанию:" />
					<label lang="uk" value="Розмірш шрифта за заомвчуванням:" />
				</item>	
				<item type="spinbox" property="DefaultFixedFontSize" default="14" minimum="5" maximum="40" step="1">
					<label lang="en" value="Default fixed font size:" />
					<label lang="ru" value="Размер фиксированного шрифта по умолчанию:" />
					<label lang="uk" value="Розмір фіксованого шрифта за замовчуванням:" />
				</item>	
			</tab>
		</tab>
	</page>
</settings>
